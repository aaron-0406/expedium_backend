model caseFiles {
  id                  Int      @id @default(autoincrement())
  code                String   @db.VarChar(45) @default("N/A")
  jurisdictional_body String   @db.VarChar(45) @default("N/A")
  judicial_district   String   @db.VarChar(45) @default("N/A")
  judge               String   @db.VarChar(100) @default("N/A")
  legal_specialist    String   @db.VarChar(100) @default("N/A")
  process_type        String   @db.VarChar(45) @default("N/A")
  observation         String   @db.VarChar(1000) @default("N/A")
  speciality          String   @db.VarChar(45) @default("N/A")
  matters             String   @db.VarChar(45) @default("N/A")
  status              String   @db.VarChar(45) @default("N/A")
  procedural_stage    String   @db.VarChar(45) @default("N/A")
  location            String   @db.VarChar(45) @default("N/A")
  conclusion_reason   String   @db.VarChar(45) @default("N/A")
  sumary              String   @db.VarChar(1000) @default("N/A")
  conclusion_date     DateTime @default(now())
  start_date          DateTime @default(now())

  created_at DateTime  @default(now())
  updated_at DateTime  @default(now())
  deleted_at DateTime?

  USER_GROUP_CASE_FILES user_group_case_files[]
  BINNACLES             binnacles[]

  @@map("CASE_FILES")
}
